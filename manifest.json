{
	"manifest_version": 2,

	"name": "Harvey",
	"description": "A chrome app to author Harvey tests.",
	"version": "1.0.1",


	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},
	"permissions": [ "*://*/*", {"fileSystem": ["write"]}],

	"minimum_chrome_version": "23",

	"sandbox": {
		"pages": ["harvey.html"]
	}
}
