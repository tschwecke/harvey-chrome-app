{
  "manifest_version": 2,

  "name": "Harvey",
  "description": "A chrome app to author Harvey tests.",
  "version": "1.0",


   "app": {
      "launch": {
         "local_path": "harvey.html"
      }
   },
   "permissions": [ "tabs", "*://*/*" ],
	"content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
